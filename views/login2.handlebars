<body class="login-body">

  <!-- Login card -->
  <div class="demo-card-event mdl-card mdl-shadow--2dp">
    <div class ="everything_else">
      <div class="mdl-card__title">
        <h1 class="title-text chorus-login">Chorus</h1>
      </div>

      <div class="login-text">Login</div>
      <!-- user name text field -->
      <div class="login-input" action="#">
        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
          <label class="mdl-textfield__label" for="username">Username</label>
          <input class="mdl-textfield__input" type="text" id="username">
        </div>
      </div>

      <!-- password text field -->
      <div class="login-input" action="#">
        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
          <label class="mdl-textfield__label" for="password">Password</label>
          <input class="mdl-textfield__input" type="password" id="password">
        </div>
      </div>
      <div class= "login">
        <!-- Log-in button -->
        <button id="login-btn" class="mdl-button mdl-js-button mdl-button--raised mdl-button--accent">
          Log in
        </button>
      </div>
      <div class= "register">
        <!-- Log-in button -->
        <a href="/register" class="mdl-button mdl-js-button mdl-button--accent">
          Register Now
        </a>
      </div>
    </div>
  </div>
